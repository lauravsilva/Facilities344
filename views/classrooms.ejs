<div class="container">
  <div class="page-header">
    <h1>Classrooms <small>All classrooms available</small></h1> </div>
  <div class="row"/>
  <div class="col-sm-12">
    <p>This will show a full list of classrooms</p>
  </div>
  <div class="container">
    <table class="table table-striped">
      <thead>
        <th>ID</th>
        <th>Building</th>
        <th>Room</th>
        <th>Capacity</th>
      </thead>

      <% if (userObjects){
      if (userObjects[0].hasOwnProperty("ClassroomID")){for(var i=0; i < userObjects.length; i++) { %>

      <tbody>
      <tr>
        <td><%= userObjects[i].ClassroomID %></td>
        <td><%= userObjects[i].Building %></td>
        <td><%= userObjects[i].RoomNumber %></td>
        <td><%= userObjects[i].Capacity %></td>
      </tr>
      </tbody>

      <% }}}%>
    </table>
  </div>
</div>
