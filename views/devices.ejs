<div class="container">
    <div class="page-header">
        <h1>Devices <small>All devices available</small></h1> </div>
        <div class="row"/>
        <div class="col-sm-12">
            <p>This will show a full list of devices</p>
        </div>
        <div class="container">
          <table class="table table-striped">
            <thead>
              <th>Course ID</th>
              <th>Name</th>
              <th>Description</th>
            </thead>

            <% if (userObjects){
            if (userObjects[0].hasOwnProperty("CourseID")){for(var i=0; i < userObjects.length; i++) { %>

            <tbody>
              <tr>
                <td><%= userObjects[i].CourseID %></td>
                <td><%= userObjects[i].Name %></td>
                <td><%= userObjects[i].Description %></td>
                <td>
                  <button type="button" class="open-reservedevice btn btn-sm" data-toggle="modal" data-target="#reserveDevice" data-device-name="<%= userObjects[i].DeviceName %>">See Roster</button>
                </td>
                <% }}}%>
              </tr>
            </tbody>
          </table>
    </div>
</div>
