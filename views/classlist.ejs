<div class="container">
    <div class="page-header">
        <h1>Classes <small>All Classes available</small></h1> </div>
        <div class="row"/>
        <div class="col-sm-12">
            <p>This will show a full list of classes</p>
        </div>
        <div class="container">
          <table class="table table-striped">
            <thead>
              <th>CourseID</th>
              <th>Name</th>
              <th>Description</th>
            </thead>

            
            <% if (userObjects){ if (userObjects[0].hasOwnProperty("CourseID")){for(var i=0; i < userObjects.length; i++) { %>

            <tbody>
              <tr>
                <td><%= userObjects[i].CourseID %></td>
                <td><%= userObjects[i].Name %></td>
                <td><%= userObjects[i].Description %></td>
                <td>
                    <form action="/roster/<%= userObjects[i].CourseID %>">
                        <input type="submit" value="See Roster" />
                    </form>
                </td>
                <% }}} %>
              </tr>
            </tbody>
          </table>
    </div>
</div>